            <element key="grade_nmin">8</element>
            <element key="grade_nmax">30</element>
            <element key="prob_template">0.1</element>
            <element key="prob_template_eval">0.8</element>
            <element key="award_lecture_answered">1000</element>
            <element key="award_lecture_aced">10000</element>
